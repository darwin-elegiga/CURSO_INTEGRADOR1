    total(a:Paciente[]){
        let contmasc:number=0;
        let contfem:number=0;

        for(let i=0; i< a.length; i++){
            if(a[i].sexo){
                contmasc++;
            }
            if(!a[i].sexo){
                contfem++;
            }
        }

        return {contfem, contmasc};
    }


    pacientes[0].total(pacientes);

console.log(pacientes[0].total(pacientes))